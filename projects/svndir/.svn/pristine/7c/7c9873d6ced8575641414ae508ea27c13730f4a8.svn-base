.navbar {
	min-height: 40px;
	background-color: #144A87;
}

.navbar-header a img{
	max-height:250%;
	max-width:100%;
	
}
.navbar-nav li {
	padding-top:5px;
	padding-bottom:5px;
}

.navbar-nav li a {
	padding: 15px;
	font-size: 14px;
	font-weight: bold;
	border-right: 1px solid #ccc;
	text-transform: uppercase;
	font-family: 'Oswald', sans-serif;
}

.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:focus, .navbar-default .navbar-nav>.active>a:hover, .navbar-nav a:hover {
	color: #7FDA7F !important;
	background-color: #003F87 !important;
}
.homepic{
	display:block;
	margin-left:auto;
	margin-right:auto;
}
body { 
	padding-top: 40px; 
}

.jumbotron { 
	background: url('http://eskipaper.com/images/spa-background-1.jpg') no-repeat; 
	min-height: 450px; 
	margin: 0;
}

.jumbotron h1 { 
	font-size: 40px; 
} 

h1, h2, h3, h4 { 
	text-transform: uppercase; 
	font-family: 'Oswald', sans-serif; 
}

.enrollment{
	text-align:center;
	padding:30px 0px 20px 0px;
	
}
.moreinfo{
	padding-top:20px;
	padding-bottom:10px;
}
.moreinfo p {
	text-align: justify;
}

footer{
	margin-bottom:0px;
	padding:20px 0 30px 0px;
	background:#615D5D;
	color:#f4f4f4;
}
.fa-color{
	color:#F2DBAC;
}
.foot a{
	color:#F2DBAC;
}

.section-top{
	padding-top:50px;
}
.section-top h4{
	font-family:"Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", "DejaVu Sans", Verdana, sans-serif;
	font-weight:400;
}
.instructors p{
	font-size:14px;
	font-family:"Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", "DejaVu Sans", Verdana, sans-serif;
	font-weight:bold;
	color:#3A60B0;
}