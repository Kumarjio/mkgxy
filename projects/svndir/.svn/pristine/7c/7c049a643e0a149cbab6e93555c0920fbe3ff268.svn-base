.navbar {
	min-height: 60px;
}
.navbar-nav li {
	padding-top:5px;
	padding-bottom:5px;
}

.navbar-nav li a { 
	padding: 14px;
	font-size: 18px; 
	font-weight: bold; 
	border-right: 1px solid #ccc; 
	text-transform: uppercase; 
	font-family: 'Oswald', sans-serif;
}
	
.navbar-nav li a span { 
	font-size: 14px;
	text-transform: none;
	font-family: arial;
	margin: 5px 0px 0px 0px;
	font-weight:normal; 
}

.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:focus, .navbar-default .navbar-nav>.active>a:hover, .navbar-nav a:hover {
    color: #fff !important;
    background-color: #0383c0 !important;
}
body { 
	padding-top: 60px;
}

.jumbotron { 
	background:url('http://www.par.com/files/1813/6628/2242/par-systems-marine-background.jpg');
