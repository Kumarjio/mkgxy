{
	    width: auto;
    height: 285px;
	margin-top: 70px;
	
}
.navbar-left a:hover, .navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:focus, .navbar-default .navbar-nav>.active>a:hover {
    color:#FFF !important;
    background-color: #3E0E0F !important;
}
