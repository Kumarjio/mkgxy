    <div role="tabpanel" class="tab-pane active" id="home">
    	<div class="row">
        <div class="col-md-12">
    	<p class="page-header">Search for home by city<button class="pull-right"> submit</button></p>
    	<label for="textfield">City *</label>
        <input type="text" name="City" id="city">
        <label for="select">Search within:</label>
    	<select name="searchradius" id="searchradius">
       	  <option value="">City Boundary</option>
    	  <option value="1 mile radius">1mile</option>
    	  <option value="2 mile radius">2mile</option>
    	  <option value="3 mile radius">3mile</option>
        </select>
        </div></div>
        <div class="row page-header"> 
         <div class="col-md-12">
          <p class="page-header"> Property Details <button class="pull-right">submit</button></p>  </div>
          <div class="col-md-4">
          <label for="price" >price </label> <br>
          <input type="number" name="range1" id="range1"> to <input type="number" name="range2" id="range2">
          </div>
          <div class="col-md-4">
          <label for="price" >Bedroom </label> <br>
          <input type="number" name="room1" id="room1"> to <input type="number" name="room2" id="room2">
          </div>
          <div class="col-md-4">
          <label for="price" >Bathrooms </label> <br>
          <input type="number" name="bath1" id="bath1"> to <input type="number" name="bath2" id="bath2">
          </div>
         </div>
      <div class="row page-header">
      <div class="col-md-2">
      	<p>Property Type</p>
       </div>
       <div class="col-md-2">
       	<input name="All" type="checkbox" id="All" checked>
        <label for="All" > All</label><br>
        <input name="Single-Family Home" type="checkbox" id="Single-Family Home">
        <label for="Single-Family Home" > Single-Family Home</label><br>
        <input name="Condo" type="checkbox" id="Condo">
        <label for="Condo" > Condo</label>
       </div>
       <div class="col-md-2">
       <input name="Townhome" type="checkbox" id="Townhome">
        <label for="Townhome" > Townhome</label><br>
        <input name="Coop" type="checkbox" id="Coop">
        <label for="Coop" > Coop</label><br>
        <input name="Loft" type="checkbox" id="Loft">
        <label for="Loft" > Loft</label>
       </div>
       <div class="col-md-2">
       <input name="TIC" type="checkbox" id="TIC">
        <label for="TIC" > TIC</label><br>
        <input name="Apt/Condo/Twnhme" type="checkbox" id="Apt/Condo/Twnhme">
        <label for="Apt/Condo/Twnhme" > Apt/Condo/Twnhme</label><br>
        <input name="Mobile/Manufactured" type="checkbox" id="Mobile/Manufactured">
        <label for="Mobile/Manufactured" > All</label>
       </div>
       <div class="col-md-2">
       <input name="Farm/Ranch" type="checkbox" id="Farm/Ranch">
        <label for="Farm/Ranch" > Farm/Ranch</label><br>
        <input name="Lot/Land" type="checkbox" id="Lot/Land">
        <label for="Lot/Land" > Lot/Land</label><br>
        <input name="Multi-familyhome" type="checkbox" id="Multi-familyhome">
        <label for="Multi-familyhome" > Multi-family Home</label>
       </div>
       <div class="col-md-2">
      <input name="Income/Invest" type="checkbox" id="Income/Invest">
        <label for="Income/Invest" > Income/Invest</label><br>
        <input name="Houseboat" type="checkbox" id="Houseboat">
        <label for="Houseboat" > Houseboat</label>
       </div>
      </div>
      <div class="row page-header">
      <div class="col-md-4">
      <label for="price" >Structure size (square feet) </label> <br>
      <input type="number" name="Structure1" id="Structure1"> to <input type="number" name="Structure2" id="Structure2">
      </div>
      <div class="col-md-4">
      <label for="price" >Lot size (acres) </label> <br>
      <input type="number" name="lot1" id="lot1"> to <input type="number" name="lot2" id="lot2">
      </div>
      <div class="col-md-4">
      <label for="price" >Year Built </label> <br>
      <input type="number" name="year1" id="year1"> to <input type="number" name="year2" id="year2">
      </div>
      </div>
      <div class="row">
      <div class="col-md-12">
      <label for="textfield">Keyword Search</label><br>
        <input type="text" name="keyword	" id="keyword">
      <p>e.g. "garage", "pool", "patio", etc.</p></div>
      </div>
      
