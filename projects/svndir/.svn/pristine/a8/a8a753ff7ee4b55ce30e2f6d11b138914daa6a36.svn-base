.navbar {
	min-height: 50px;
}

.navbar-header a img{
	max-height:300%;
	max-width:100%;
}
.navbar-nav li {
	padding-top:5px;
	padding-bottom:5px;
}

.navbar-nav li a { 
	padding: 20px;
	font-size: 18px; 
	font-weight: bold; 
	border-right: 1px solid #ccc; 
	text-transform: uppercase; 
	font-family: 'Oswald', sans-serif;
}
	
.navbar-nav li a span { 
	font-size: 14px;
	text-transform: none;
	font-family: arial;
	margin: 5px 0px 0px 0px;
	font-weight:normal; 
}

.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:focus, .navbar-default .navbar-nav>.active>a:hover, .navbar-nav a:hover {
    color: #fff !important;
    background-color: #CD853F !important;
}
body { 
	padding-top: 50px; 
}

.jumbotron { 
	background: url('http://eskipaper.com/images/spa-background-1.jpg') no-repeat; 
	min-height: 450px; 
	margin: 0;
}

.jumbotron h1 { 
	font-size: 50px; 
} 

h1, h2, h3 { 
	text-transform: uppercase; 
	font-family: 'Oswald', sans-serif; 
}

.section-gray{
	background-color: #f4f4f4;
}

section{
	padding: 30px 0px 30px 0px;
}

.section-secondary{
	color:#fff;
	background-color:#CD853F;
	padding: 60px 0px 60px 0px;
}
.slogan{
	text-align:center;
}
.slogan h1{
	font-size:45px;
	margin-bottom:25px;
}
.slogan p{
	font-size:20px;
}
.slogan a{
	margin-top:20px;
}
