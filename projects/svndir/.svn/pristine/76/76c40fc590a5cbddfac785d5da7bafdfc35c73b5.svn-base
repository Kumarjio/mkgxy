    border-radius: 0px;
    padding-top: 15px;
    padding-bottom: 15px;
    font-size: 16px;
    font-weight: bold;
    font-family: 'Open Sans', sans-serif;
}
.navbar-nav li a{
   color: yellowgreen;
   
}

body { 
